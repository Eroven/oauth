{
  "groups": [
    {
      "name": "jwt",
      "type": "me.zhaotb.oauth.server.bean.JwtConfig",
      "sourceType": "me.zhaotb.oauth.server.bean.JwtConfig"
    },
    {
      "name": "mock",
      "type": "me.zhaotb.oauth.server.config.MockDataConfig",
      "sourceType": "me.zhaotb.oauth.server.config.MockDataConfig"
    }
  ],
  "properties": [
    {
      "name": "jwt.access-token-expiration",
      "type": "java.lang.Long",
      "description": "AccessToken有效时长（毫秒），默认1小时",
      "sourceType": "me.zhaotb.oauth.server.bean.JwtConfig",
      "defaultValue": 0
    },
    {
      "name": "jwt.patterns",
      "type": "java.util.List<java.lang.String>",
      "description": "匹配需要校验的链接.注意前后不要留空格。与servlet的pattern格式一致，支持*和**通配",
      "sourceType": "me.zhaotb.oauth.server.bean.JwtConfig"
    },
    {
      "name": "jwt.refresh-token-expiration",
      "type": "java.lang.Long",
      "description": "refreshToken有效时长（毫秒），默认1天",
      "sourceType": "me.zhaotb.oauth.server.bean.JwtConfig",
      "defaultValue": 0
    },
    {
      "name": "jwt.secret",
      "type": "java.lang.String",
      "description": "密钥",
      "sourceType": "me.zhaotb.oauth.server.bean.JwtConfig"
    },
    {
      "name": "mock.domain",
      "type": "java.lang.String",
      "description": "域名",
      "sourceType": "me.zhaotb.oauth.server.config.MockDataConfig"
    },
    {
      "name": "mock.register-client",
      "type": "java.util.Map<java.lang.String,java.lang.String>",
      "description": "注册过的第三方客户端",
      "sourceType": "me.zhaotb.oauth.server.config.MockDataConfig"
    },
    {
      "name": "mock.user-account",
      "type": "me.zhaotb.oauth.server.entity.UserAccount",
      "description": "用户信息",
      "sourceType": "me.zhaotb.oauth.server.config.MockDataConfig"
    }
  ],
  "hints": []
}